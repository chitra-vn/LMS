<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title-Library</title>
    <link href="~/Bootstrap/css/bootstrap.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Bootstrap/font-awesome/css/font-awesome.min.css" rel="stylesheet" />

    <link rel="stylesheet" media="all" type="text/css" href="~/Scripts/css/jquery-ui.css" />

    @*<link href="~/Bootstrap/css/bootstrap-select.min.css" rel="stylesheet" />*@
    <link href="~/Content/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Scripts/css/datatables.css" rel="stylesheet" />
    <!-- jQuery -->

    
    <script src="~/Scripts/js/jquery.min.js"></script>
    <script src="~/Scripts/js/datatables.js"></script>
    <script src="~/Scripts/js/dataTables.bootstrap.min.js"></script>
    <script src="~/Scripts/js/validator.min.js"></script>
    @RenderSection("JavaScript", required: false)

</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a href="~/BookLibraryHome/Index" class="navbar-brand"><span>Books Library</span></a>
                @*Html.ActionLink("Books Library", "Index", "BookLibraryHome", new { area = "" }, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p style="margin-top:33px">&copy; @DateTime.Now.Year - Books Library</p>
        </footer>
    </div>

    @*<script src="~/Scripts/jquery-1.10.2.min.js"></script>*@
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>